{
	"diagnostic.virtualText": true,
	"diagnostic.autoRefresh": true,
	"diagnostic.infoSign": "",
	"diagnostic.errorSign": "",
	"diagnostic.hintSign": "",
	"diagnostic.warningSign": "",
	"diagnostic.messageTarget": "echo",
	"signature.enable": true,
	"suggest.autoTrigger": "always",
	"suggest.enablePreview": true,
	"suggest.completionItemKindLabels": {
		"keyword": "",
		"variable": "",
		"value": "",
		"operator": "",
		"function": "",
		"reference": "",
		"constant": "",
		"method": "",
		"struct": "פּ",
		"class": "ﴯ",
		"interface": "",
		"text": "",
		"enum": "",
		"enumMember": "",
		"module": "",
		"color": "",
		"property": "ﰠ",
		"field": "ﰠ",
		"unit": "塞",
		"event": "",
		"file": "",
		"folder": "",
		"snippet": "",
		"typeParameter": "\uf728",
		"default": "\uf29c"
	},
	"coc.preferences.formatOnType": true,
	"coc.preferences.echodocSupport": true,
	"coc.preferences.formatOnSaveFiletypes": [
		"python",
		"markdown",
		"json",
		"csv"
	],
	//python
	"pyright.enable": true,
	"python.linting.enabled": true,
	"pyright.disableDiagnostics": false,
	"python.linting.pylintEnabled": true,
	"python.pythonPath": "~/.vim/plugged/coc.nvim/python",
	//markdown
	"markdownlint.onOpen": true,
	"markdownlint.onChange": true,
	"markdownlint.onSave": true,
	"markdownlint.config": {
		"default": true,
		"line_length": false
	}
}
